<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="fashionmanager.kim.develop.mapper.ReportCategoryMapper">
    <resultMap id="reportCategoryMap" type="fashionmanager.kim.develop.dto.ReportCategoryDTO">
        <id property="reportCategoryNum" column="NUM"/>
        <result property="reportCategoryName" column="NAME"/>
    </resultMap>

    <select id="selectAllReportCategories" resultMap="reportCategoryMap">
        SELECT
               A.NUM
             , A.NAME
        FROM REPORT_CATEGORY A
    </select>
</mapper>